                    <input type="text" ng-model="surity.worthofselfimmovableproperty" class="form-control search-orders" placeholder="Asset worth of self owned Immovable Property">
                </div>
                <div class="form-group mb-3 col-md-3">
                    <label for="witnessname" class="form-label">Witness Name</label>
                    <input type="text" ng-model="surity.witnessname" class="form-control search-orders" placeholder="Witness Name">
                </div>
                <div class="form-group mb-3 col-md-3">
                    <label for="witnessaddress" class="form-label"> Witness Address </label>
                    <textarea id="witnessaddress" ng-model="surity.witnessaddress" class="form-control" placeholder="Witness Address"></textarea>
                </div>
                @* </div> *@
                <div class="text-center m-2 col-md-4">
                    <button type="submit" class="btn" style="background-color:#003980; color:white;">
                        ADD
                    </button>
                </div>
            </div>
