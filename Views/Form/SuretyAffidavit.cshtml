<body style="padding: 5% 8% 0 8%;" ng-controller="FormController">
    <div class="row align-items-center">
        <!-- Arrow Icon aligned to the left -->
        <a class="col-md-1 text-start" style="font-size:35px;" ng-click="navtogenerateform()">
            <i class="bi bi-arrow-left"></i>
        </a>

        <!-- Centered Heading -->
        <div class="col-md-10 text-center">
            <h3>Surety affidavit</h3>
        </div>
    </div>
    <div ng-if="hDiv">
        <form name="gForm" ng-submit="addSurityData()">
            <div class="row">
                @* <div class="col-md-4"> *@
                <div class="form-group mb-3">
                    <label for="seletedid" class="form-label">select customer</label>
                    <select class="form-select w-auto" ng-options="f.id as f.name for f in docs" ng-model="seletedid" ng-change="loadCustomers(seletedid)">
                        <option selected disabled value="">Select customer</option>
                    </select>
                </div>
                <div class="form-group mb-3 col-md-3">
                    <label for="suretyname" class="form-label">Surity Name</label>
                    <input type="text" ng-model="surity.suretyname" class="form-control search-orders" placeholder="Surity Name">
                </div>
                <div class="form-group mb-3 col-md-3">
                    <label for="age" class="form-label">Age</label>
                    <input type="text" ng-model="surity.age" class="form-control search-orders" placeholder="Age">
                </div>
                <div class="form-group mb-3 col-md-3">
                    <label for="suretyaddress" class="form-label"> Surity Address </label>
                    <textarea id="suretyaddress" ng-model="surity.suretyaddress" class="form-control" placeholder="Surity Address"></textarea>
                </div>
                <div class="form-group mb-3 col-md-3">
                    <label for="knowtheapplicant" class="form-label">Know The Applicant in Years</label>
                    <input type="text" ng-model="surity.knowtheapplicant" class="form-control search-orders" placeholder="Know The Applicant in Years">
                </div>
                <div class="form-group mb-3 col-md-3">
                    <label for="bussinessaddress" class="form-label">Bussiness Address</label>
                    <textarea id="bussinessaddress" ng-model="surity.bussinessaddress" class="form-control" placeholder="Bussiness Address"></textarea>
                </div>
                <div class="form-group mb-3 col-md-3">
                    <label for="panno" class="form-label">PAN No</label>
                    <input type="text" ng-model="surity.panno" class="form-control search-orders" placeholder="PAN No">
                </div>
                <div class="form-group mb-3 col-md-3">
                    <label for="netassetsworth" class="form-label">Net Assets Worth</label>
                    <input type="text" ng-model="surity.netassetsworth" class="form-control search-orders" placeholder="Net Assets Worth">
                </div> 
                <div class="form-group mb-3 col-md-3">
                    <label for="annualsalaryincome" class="form-label">Annual Salary Income</label>
                    <input type="text" ng-model="surity.annualsalaryincome" class="form-control search-orders" placeholder="Annual Salary Income">
                </div>
                <div class="form-group mb-3 col-md-3">
                    <label for="employername" class="form-label">Employer Name</label>
                    <input type="text" ng-model="surity.employername" class="form-control search-orders" placeholder="Employer Name">
                </div>
                <div class="form-group mb-3 col-md-3">
                    <label for="selfbussinessannualincome" class="form-label">Self Owned Bussiness Annual Income</label>
                    <input type="text" ng-model="surity.selfbussinessannualincome" class="form-control search-orders" placeholder="Self Owned Bussiness Annual Income">
                </div> 
                <div class="form-group mb-3 col-md-3">
