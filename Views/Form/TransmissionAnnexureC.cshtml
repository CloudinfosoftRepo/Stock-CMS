<script>
    angular.module('Stock-CMS', []).controller('FormController', function ($scope, $http,$timeout) {
        $scope.customers = [];
    $scope.data = undefined;
              $scope.hDiv = true;
              var params = new URLSearchParams(window.location.search);
              var id = params.get('id');
              var id2 = params.get('id2');
              $scope.isPrint = id2 ? true : false

        var isProcessing = false;

        // showLoader();

        $scope.radiocheck = function(val){
            if(val == 'option4'){
                $scope.doclist.option5 = false;
            }
            else{
                $scope.doclist.option4 = false;
            }
            
            $scope.loadCustomers();
        } 

        $scope.radiocheck1 = function(val){
            if(val == 'option6'){
                $scope.doclist.option7 = false;
            }
            else{
                $scope.doclist.option6 = false;
            }
            $scope.loadCustomers();
        }

                 // Load Customers
         $scope.loadCustomers = function (sid,hid) {
             if(id){
                $scope.hDiv = true;
                $http.get('/Enquiry/GetStocksById/'+id).then(function (response) {
                    var result = response.data;
                    $http.get('/Form/GetHolderByStockId/' +  id).then(function (response) {
                        $scope.docs = response.data;
                        var doc = $scope.docs.find(x => x.id === sid);
                        $http.get('/LegalHeir/GetClaimentLegalHeir/' + doc.id).then(function (response) {
                            $scope.heir =response.data;
                            var heirs = response.data.find(x => x.id === hid);
                            $http.get('/doc/GetLegalHeirBank/'+ heirs.id).then(function (response) {
                                var bank = response.data[0];
                               
                                $scope.data = {
                                    customerName : heirs.nameAsPerAadhar,
                                    rtaName: result.company.rtaName,
                                    rtaAddress: result.company.rtaAddress,
                                    companyName: result.companyName,
                                    pan: heirs.pan,
                                    kyc: $scope.data && $scope.data.kyc || 'KYC',
                                    resident: $scope.data && $scope.data.resident || '',
                                    relation: $scope.data && $scope.data.relation || '',
                                    nameofdeacesed: doc.name,
                                    dateofdeath: doc.dateOfDeath,
                                    folioNo: result.folioNo,
                                    qty: result.qty,
                                    rate: result.rate,
                                    mobile: heirs.mobile,
                                    email: heirs.email,
                                    address: heirs.addressAsPerAadhar,
                                    bankName: bank.bankName,
                                    accountNo: bank.accountNo,
                                    ifsccode: bank.ifsccode,
                                    accountType: bank.accountType,
                                    micrcode: bank.micrcode,
                                    branch: bank.branch,
                                    bankattachfile: $scope.data && $scope.data.bankattachfile || '',
                                    occupation: $scope.data && $scope.data.occupation || '',
                                    politicalrelation: $scope.data && $scope.data.politicalrelation || '',
                                    income: $scope.data && $scope.data.income || '',
                                    countryofbirth: $scope.data && $scope.data.countryofbirth || 'INDIA',
                                    placeofbirth: $scope.data && $scope.data.placeofbirth || '',
                                    nationality: $scope.data && $scope.data.nationality || 'INDIAN',
                                    taxresident: $scope.data && $scope.data.taxresident || '',
                                    taxcountry: $scope.data && $scope.data.taxcountry || '',
                                    taxidnumber: $scope.data && $scope.data.taxidnumber || '',
                                    identitytype: $scope.data && $scope.data.identitytype || '',
                                    place: $scope.data && $scope.data.place || 'Vadodara',
                                    today: new Date(),
                                };

                                hideLoader();
                            }, function (error) {
                            console.error('Error loading Bank Deatails:', error);
                            hideLoader();
                            });
                        }, function (error) {
                        console.error('Error loading legal Heir:', error);
                        hideLoader();
                        });
                    }, function (error) {
                    console.error('Error loading Docs:', error);
                    hideLoader();
                    });
                }, function (error) {
                console.error('Error loading customers:', error);
                hideLoader();
                });
            }

            if(id2){
                 $scope.hDiv = false;
            $http.get('/Form/GetGenratedFormById/'+id2).then(function (response) {
                var result = response.data;
                var obj = JSON.parse(result.jsonString);
                $scope.data = {
                    customerName: obj.customerName,
                    rtaName: obj.rtaName,
                    rtaAddress: obj.rtaAddress,
                    companyName: obj.companyName,
                    pan: obj.pan,
                    kyc: obj.kyc,
                    resident: obj.resident,
                    relation: obj.relation,
                    nameofdeacesed: obj.nameofdeacesed,
                    dateofdeath: obj.dateofdeath,
                    folioNo: obj.folioNo,
                    qty: obj.qty,
                    rate: obj.rate,
                    mobile: obj.mobile,
                    email: obj.email,
                    address: obj.address,
                    bankName: obj.bankName,
                    accountNo: obj.accountNo,
                    ifsccode: obj.ifsccode,
                    accountType: obj.accountType,
                    micrcode: obj.micrcode,
                    branch: obj.branch,
                    bankattachfile: obj.bankattachfile,
                    occupation: obj.occupation,
                    politicalrelation: obj.politicalrelation,
                    income: obj.income,
                    countryofbirth: obj.countryofbirth,
                    placeofbirth: obj.placeofbirth,
                    nationality: obj.nationality,
                    taxresident: obj.taxresident,
                    taxcountry: obj.taxcountry,
                    taxidnumber: obj.taxidnumber,
                    identitytype: obj.identitytype,
                    place: obj.place,
                    today: new Date(obj.today),
                    docattach: JSON.parse(obj.docattach),
                };
                $scope.doclist = $scope.data.docattach;
                id = result.stockId;
                checkData();
                hideLoader();
            }, function (error) {
                console.error('Error loading customers:', error);
                hideLoader();
            });
            }
        };

    function checkData() {
        if ($scope.data !== undefined) {
            // Delay the printDiv execution until the next digest cycle
            $timeout(function() {
