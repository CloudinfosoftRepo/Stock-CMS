        <script>
    angular.module('Stock-CMS', []).controller('FormController', function ($scope, $http, $timeout) {
        $scope.customers = [];
        $scope.data = undefined;
                $scope.hDiv = true;
                var params = new URLSearchParams(window.location.search);
                var id = params.get('id');
                var id2 = params.get('id2');
                $scope.checkboxes = {
                    pan: false,
                    bankDetails: false,
                    signature: false,
                    mobileNumber: false,
                    emailId: false,
                    address: false
                };
                $scope.checkboxes1 = {
                    option1: false,
                    option2: false,
                    option3: false,
                    option4: false,
                    option5: false,
                    option6: false,
                }
                $scope.isPrint = id2 ? true : false
        var isProcessing = false;
        $scope.processdone = false;

        // showLoader();

                // Load Customers
         $scope.loadCustomers = function ( hid) {
    if (id) {
        var url = '';
        $scope.hDiv = true;

        $http.get('/Enquiry/GetStocksById/' + id).then(function (response) {
            var result = response.data;

            $http.get('/Form/GetHolderByStockId/' + id).then(function (response) {
                $scope.docs = response.data;
                // var doc = $scope.docs.find(x => x.id === sid);

                // Determine alive status of holders safely
                var firstAlive = result.firstHolderData && !result.firstHolderData.dateOfDeath;
                var secondAlive = result.secondHolderData && !result.secondHolderData.dateOfDeath;
                var thirdAlive = result.thirdHolderData && !result.thirdHolderData.dateOfDeath;

                var isDeceased = !(firstAlive || secondAlive || thirdAlive);

                // Pick the first alive holder or null if none alive
                var holder;
                if (firstAlive) {
                    holder = result.firstHolderData;
                } else if (secondAlive) {
                    holder = result.secondHolderData;
                } else if (thirdAlive) {
                    holder = result.thirdHolderData;
                } else {
                    holder = null;
                }

                if (isDeceased) {
                    // All holders deceased, proceed with legal heir of first holder
                    $scope.heirId = null;
                    $scope.legalheirlist = isDeceased;
                    document.getElementById('heirId').value = '';

                    $http.get('/LegalHeir/GetClaimentLegalHeir/' + result.firstHolderId).then(function (response) {
                        $scope.heir = response.data;
                        var heirs = response.data.find(x => x.id === hid); // hid should be in scope
                        var heirHolder = heirs;

                        $http.get('/doc/GetLegalHeirBank/' + heirs.id).then(function (response) {
                            var bank = response.data[0];
                            populateData(heirHolder, result, $scope.heir, isDeceased, bank);
                        }, function (error) {
                            console.error('Error loading Bank Details:', error);
                            hideLoader();
                        });
                    }, function (error) {
                        console.error('Error loading legal heir data:', error);
                        hideLoader();
                    });

                } else {
                    // At least one holder alive, proceed with that holder's data
                    $scope.heirId = null;
                    $scope.data = null;
                    $scope.legalheirlist = isDeceased;
                    document.getElementById('heirId').value = '';

                    if (!holder) {
                        console.error('No alive holder found despite isDeceased being false.');
                        hideLoader();
                        return;
                    }

                    $http.get('/doc/GetBank/' + holder.id).then(function (response) {
                        var bank = response.data[0];
                        populateData(holder, result, $scope.heir, isDeceased, bank);
                    }, function (error) {
                        console.error('Error loading Bank Details:', error);
                        hideLoader();
                    });
                }

                hideLoader();

            }, function (error) {
                console.error('Error loading Docs:', error);
                hideLoader();
            });

        }, function (error) {
            console.error('Error loading customers:', error);
            hideLoader();
        });
    }

    if (id2) {
        $scope.hDiv = false;
        $http.get('/Form/GetGenratedFormById/' + id2).then(function (response) {
            var result = response.data;
            var obj = JSON.parse(result.jsonString);

            $scope.data = {
                docid: obj.docid,
                customerName: obj.customerName,
                today: obj.today,
                updatecheck: JSON.parse(obj.updatecheck),
                companyName: obj.companyName,
                folioNo: obj.folioNo,
                firstHolder: obj.firstHolder,
                secondHolder: obj.secondHolder,
                thirdHolder: obj.thirdHolder,
                qty: obj.qty,
                rate: obj.rate,
                jsondata: obj.jsondata,
                distinctiveNosStart: obj.distinctiveNosStart,
                distinctiveNosEnd: obj.distinctiveNosEnd,
                selectedOption: obj.selectedOption,
                prooffroaddress: JSON.parse(obj.prooffroaddress),
                accountno: obj.accountno,
                bankname: obj.bankname,
                branch: obj.branch,
                ifsccode: obj.ifsccode,
                email: obj.email,
                mobile: obj.mobile,
                newemail: obj.newemail,
                newmobile: obj.newmobile,
                firstHolderaddress: obj.firstHolderaddress,
                secondHolderaddress: obj.secondHolderaddress,
                thirdHolderaddress: obj.thirdHolderaddress,
                firstHolderpincode: obj.firstHolderpincode,
                secondHolderpincode: obj.secondHolderpincode,
                thirdHolderpincode: obj.thirdHolderpincode,
                faceValue: obj.faceValue,
                condition: obj.condition,
                heirs: obj.heirs,
                legelheirpan: obj.legelheirpan,
            };

            $scope.checkboxes = $scope.data.updatecheck;
            $scope.checkboxes1 = $scope.data.prooffroaddress;
            $scope.shares = JSON.parse($scope.data.jsondata);
            id = result.stockId;
            checkData();
            hideLoader();

        }, function (error) {
            console.error('Error loading customers:', error);
            hideLoader();
        });
    }

    function populateData(holder, result, heirlist, isDeceased, bank) {
        $scope.data = {
            docid: holder.id,
            customerName: holder.nameAsPerAadhar,
            today: new Date(),
            companyName: result.companyName,
            folioNo: result.folioNo,
            firstHolder: result.firstHolderName,
            secondHolder: result.secondHolderName,
            thirdHolder: result.thirdHolderName,
            firstHolderpan: result.firstHolderData ? result.firstHolderData.pan : '',
            secondHolderpan: result.secondHolderData ? result.secondHolderData.pan : '',
            thirdHolderpan: result.thirdHolderData ? result.thirdHolderData.pan : '',
            legelheirpan: holder.pan,
            qty: result.qty,
            rate: result.rate,
            distinctiveNosStart: $scope.data && $scope.data.distinctiveNosStart || '',
            distinctiveNosEnd: $scope.data && $scope.data.distinctiveNosEnd || '',
            selectedOption: $scope.data && $scope.data.selectedOption || 'Yes',
            accountno: bank.accountNo,
            bankname: bank.bankName,
            branch: bank.branch,
            ifsccode: bank.ifsccode,
            email: holder.email,
            mobile: holder.mobile,
            newemail: $scope.data && $scope.data.newemail || '',
            newmobile: $scope.data && $scope.data.newmobile || '',
            firstHolderaddress: result.firstHolderData ? result.firstHolderData.addressAsPerAadhar : '',
            secondHolderaddress: result.secondHolderData ? result.secondHolderData.addressAsPerAadhar : '',
            thirdHolderaddress: result.thirdHolderData ? result.thirdHolderData.addressAsPerAadhar : '',
            firstHolderpincode: result.firstHolderData ? result.firstHolderData.pinCode : '',
            secondHolderpincode: result.secondHolderData ? result.secondHolderData.pinCode : '',
            thirdHolderpincode: result.thirdHolderData ? result.thirdHolderData.pinCode : '',
            faceValue: result.faceValue,
            jsondata: result.stockJson,
            condition: isDeceased,
            heirs: isDeceased ? heirlist.map(h => ({ name: h.name, address: h.addressAsPerAadhar, pincode: h.pinCode })) : [],
        };
        $scope.shares = JSON.parse($scope.data.jsondata);
    }
};


        function checkData() {
        if ($scope.data !== undefined) {
            // Delay the printDiv execution until the next digest cycle
            $timeout(function() {
                $scope.printDiv('printForm');
            }, 0);  // 0 delay to ensure it runs after the view update
        } else {
            setTimeout(checkData, 1000); // Retry after 1000ms if data is not yet available
        }
    }

     //Genrate Form
         $scope.genrateForm = function () {
             if(id){
                 $scope.processdone = true;
                  if($scope.data.jsondata == undefined){
                     Swal.fire({
                            title: 'Error!',
                            text:  'Please Enter Stock Qty data in Custome\'s Stock Page',
                            icon: 'error',
                            confirmButtonText: 'Try Again'
                        });
                        $scope.processdone = false;
                return;
                 }
                 var arr = angular.copy($scope.checkboxes);
                 var arr1 = angular.copy($scope.checkboxes1);
                    $scope.data.updatecheck=  JSON.stringify(arr,null,2);
                    $scope.data.prooffroaddress= JSON.stringify(arr1,null,2);


             let jsonString = JSON.stringify($scope.data, null, 2);
             var data ={
                 formName : 'Form ISR – 1',
                 StockId : id,
                 url : `/Form/FormISR1?id2=`,
                 jsonString : jsonString,
                 ClientName: $scope.data.customerName,
                  ClientId: $scope.data.docid,
                 IsActive : true
             }

             const url = '/Form/GenrateForm';
                $http.post(url, data).then(function (response) {
                    if (response.data.success) {
                        Swal.fire({
                            title: 'Success',
                            text: response.data.message || 'Operation successful!',
                            icon: 'success',
                            confirmButtonText: 'OK'
                        });
                        $scope.processdone = false;
                        // $scope.loadCustomers();
                        window.open(response.data.data.url + response.data.data.id,'_self')

                    } else {
                        Swal.fire({
                         title: 'This Form is already Exits',
                         text: "do you want to go to that form?",
                         icon: 'warning',
                         showCancelButton: true,
                         confirmButtonText: 'Yes',
                         cancelButtonText: 'No'
                     }).then((result) => {
                         if (result.isConfirmed) {
                             window.open(response.data.message, '_self');
                         }
                 });
                 $scope.processdone = false;
                    }
                    isProcessing = false;
                }, function (error) {
                    Swal.fire({
                        title: 'Error',
